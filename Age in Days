#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int n, y, m, d;
    cin >> n;

    y = n / 365;
    cout << y << " " << "years" << endl;
    n = n % 365;

    m = n / 30;
    cout << m << " " << "months" << endl;

    d = n % 30;
    cout << d << " " << "days" << endl;

    return 0;
}
