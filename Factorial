#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n, x, i, j;
    cin >> n;

    long long int fact;

    for(i = 1; i <= n; i++)
    {
        cin >> x;
        fact = 1;

        for(j = 1; j <= x; j++)
        {
            fact = fact * j;
        }

        cout << fact << endl;
    }

    return 0;
}
