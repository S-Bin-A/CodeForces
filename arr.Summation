#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    long long int n, sum = 0;
    cin >> n;
 
    vector <long long int> arr(n);
 
    for(long long int i = 0; i < n; i++)
    {
        cin >> arr[i];
        sum += arr[i];
    }
 
    if(sum >= 0)
        cout << sum << endl;
    else if(sum < 0)
    {
        sum = -sum;
        cout << sum << endl;
    }
 
    return 0;
}
