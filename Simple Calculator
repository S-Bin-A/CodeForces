
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x;
    long long int y, sum, mul, sub;
 
    cin >> x;
    cin >> y;
 
    sum = x + y;
    mul = x*y;
    sub = x - y;
 
    cout << x << " " << '+' << " " << y << " " << '=' << " " << sum << endl;
    cout << x << " " << '*' << " " << y << " " << '=' << " " << mul << endl;
    cout << x << " " << '-' << " " << y << " " << '=' << " " << sub << endl;
 
    return 0;
}
