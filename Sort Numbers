#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int a, b, c, temp;
    cin >> a >> b >> c;

    long long int m = a, n = b, o = c;

    if(a < b)
    {
        temp = a;
        a = b;
        b = temp;
    }

    if(a < c)
    {
        temp = a;
        a = c;
        c = temp;
    }

    if(b < c)
    {
        temp = b;
        b = c;
        c = temp;
    }

    cout << c << endl << b << endl << a << endl;
    cout << endl << m << endl << n << endl << o << endl;

    return 0;
}
